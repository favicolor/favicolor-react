{
  "name": "@favicolor/react",
  "version": "1.0.0",
  "description": "React components for Favicolor - Extract and display website favicons with dominant colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "favicon",
    "colors",
    "icons",
    "ui",
    "favicolor"
  ],
  "author": "favicolor",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/favicolor/favicolor-react"
  },
  "homepage": "https://github.com/favicolor/favicolor-react#readme",
  "bugs": {
    "url": "https://github.com/favicolor/favicolor-react/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
